{
  "name": "camptocamp-motd",
  "version": "0.1.4",
  "author": "Camptocamp",
  "summary": "Puppet motd module",
  "license": "Apache-2.0",
  "source": "https://github.com/camptocamp/puppet-motd",
  "project_page": "https://github.com/camptocamp/puppet-motd",
